{
  "name": "@danny-ui/react-form",
  "version": "0.1.2",
  "author": "Danny Qu (https://github.com/danny8903)",
  "license": "MIT",
  "keywords": [
    "form",
    "react",
    "typescript"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "types",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:types && yarn build:rollup",
    "build:types": "rm -rf types && tsc --build ./tsconfig.build.types.json",
    "build:rollup": "rm -rf lib/ && rm -rf es/ && rollup -c rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "surge --project ./styleguide --domain danny-ui-form.surge.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:danny8903/react-form.git"
  },
  "homepage": "git+https://github.com:danny8903/react-form.git",
  "bugs": {
    "url": "git+https://github.com:danny8903/react-form.git"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "testRegex": ".*/__tests__/.+\\.(test|spec)\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.css$": "<rootDir>/cssTransform.js"
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "sideEffects": false,
  "peerDependencies": {
    "lodash": ">=4.17.15",
    "react": ">=16.8.x",
    "react-dom": ">=16.8.x",
    "rxjs": ">=6.5.x"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.4",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.6.0",
    "jest": "^26.4.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-watch-typeahead": "^0.6.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-styleguidist": "^11.0.8",
    "rollup": "^2.26.7",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-typescript": "^1.0.1",
    "rxjs": "^6.5.5",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.1",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1"
  }
}
